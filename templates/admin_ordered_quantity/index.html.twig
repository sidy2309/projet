{% extends 'base.html.twig' %}

{% block title %}OrderedQuantity index{% endblock %}

{% block body %}
    <h1>OrderedQuantity index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Quantity</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for ordered_quantity in ordered_quantities %}
            <tr>
                <td>{{ ordered_quantity.id }}</td>
                <td>{{ ordered_quantity.quantity }}</td>
                <td>
                    <a href="{{ path('admin_ordered_quantity_show', {'id': ordered_quantity.id}) }}">show</a>
                    <a href="{{ path('admin_ordered_quantity_edit', {'id': ordered_quantity.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('admin_ordered_quantity_new') }}">Create new</a>
{% endblock %}
